		turbinePercentBar += 20;
		temp = turbinePercentBar;
		turbineQuantity = (totalTurbine*(temp / 100));
		turbineEmish = ((turbineQuantity)*.003948);
	}

	//conversions here
	char Qbuffer[50];
	sprintf(Qbuffer, "%.1f Thousands", turbineQuantity);
	char Ebuffer[50];
	sprintf(Ebuffer, "%.1f Billion Metric Tons / year", turbineEmish);
	auto emishBar = std::to_string(turbineEmish);
	String^ str1 = gcnew String(Ebuffer);
	auto progBar = std::to_string(turbinePercentBar) + "%";
	String^ str2 = gcnew String(progBar.c_str());
	String^ str3 = gcnew String(Qbuffer);

	// Percent, Quantity, & Emissions lables Update
	label5->Text = str2;
	label10->Text = str3;
	label16->Text = str1;

}

	

// Wind Turbines (-)
private: System::Void button9_Click(System::Object^  sender, System::EventArgs^  e) {
	double temp;
	if (progressBar5->Value != 0) {
		progressBar5->Value -= 10;
		series5->Points->Remove(dataPoint5);
		dataPoint5 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar5->Value * 2));
		series5->Points->Add(dataPoint5);
		chartArea1->RecalculateAxesScale();
		chart1->Refresh();

		if (TotalPollution->Value != 100) {
			TotalPollution->Value += 2;
		}
		// Percent, Quantity, & Emissions value Update
		turbinePercentBar -= 20;
		temp = turbinePercentBar;
		turbineQuantity = (totalTurbine*(temp / 100));
		turbineEmish = ((turbineQuantity) * .003948);
	}
	 

	//conversions here
	char Qbuffer[50];
	sprintf(Qbuffer, "%.1f Thousands", turbineQuantity);
	char Ebuffer[50];
	sprintf(Ebuffer, "%.1f Billion Metric Tons / year", turbineEmish);
	auto emishBar = std::to_string(turbineEmish);
	String^ str1 = gcnew String(Ebuffer);
	auto progBar = std::to_string(turbinePercentBar) + "%";
	String^ str2 = gcnew String(progBar.c_str());
	String^ str3 = gcnew String(Qbuffer);

	// Percent, Quantity, & Emissions lables Update
	label5->Text = str2;
	label10->Text = str3;
	label16->Text = str1;
		
}
private: System::Void label12_Click(System::Object^  sender, System::EventArgs^  e) {
}
private: System::Void chart1_Click(System::Object^  sender, System::EventArgs^  e) {
	
}
