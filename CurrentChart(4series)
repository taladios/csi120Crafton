// Cars (+)
const double totalCar = 1.015;
double carsQuantity;
int percentBar = 100;
double emish;

	private: System::Void button1_Click_1(System::Object^  sender, System::EventArgs^  e) {
		double temp;
		if (progressBar1->Value != 100) {
			progressBar1->Value += 10;
			series1->Points->Remove(dataPoint1);
			dataPoint1 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar1->Value * 2));
			series1->Points->Add(dataPoint1);
			chartArea1->RecalculateAxesScale();
			chart1->Refresh();
			if (TotalPollution->Value != 100) {
				TotalPollution->Value += 2;
			}
			// Percent, Quantity, & Emissions value Update
			percentBar += 20;
			temp = percentBar;
			carsQuantity = (totalCar*(temp / 100));
			emish = (carsQuantity)*4.6;
		}

		//conversions here
		char Qbuffer[50];
		sprintf(Qbuffer, "%.3f Billion", carsQuantity);
		char Ebuffer[50];
		sprintf(Ebuffer, "%.3f Billion Metric Tons / year", emish);
		auto emishBar = std::to_string(emish) + "Billion Metric Tons / year";
		String^ str1 = gcnew String(Ebuffer);
		auto progBar = std::to_string(percentBar) + "%";
		String^ str2 = gcnew String(progBar.c_str());
		String^ str3 = gcnew String(Qbuffer);

		// Percent, Quantity, & Emissions lables Update
		label1->Text = str2;
		label4->Text = str3;
		label12->Text = str1;
	}
private: System::Void progressBar1_Click(System::Object^  sender, System::EventArgs^  e) {

}
// Cars (-)
private: System::Void button2_Click(System::Object^  sender, System::EventArgs^  e) {
	double temp;
	if (progressBar1->Value != 0) {
		progressBar1->Value -= 10;
		series1->Points->Remove(dataPoint1);
		dataPoint1 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar1->Value * 2));
		series1->Points->Add(dataPoint1);
		chartArea1->RecalculateAxesScale();
		chart1->Refresh();

		if (TotalPollution->Value != 0) {
			TotalPollution->Value -= 2;
		}
		// Percent, Quantity, & Emissions value Update
		percentBar -= 20;
		temp = percentBar;
		carsQuantity = (totalCar*(temp / 100));
		emish = (carsQuantity)*4.6;
	}
	//conversions here
	char Qbuffer[50];
	sprintf(Qbuffer, "%.3f Billion", carsQuantity);
	char Ebuffer[50];
	sprintf(Ebuffer, "%.3f Billion Metric Tons / year", emish);
	auto emishBar = std::to_string(emish) + "Billion Metric Tons / year";
	String^ str1 = gcnew String(Ebuffer);
	auto progBar = std::to_string(percentBar) + "%";
	String^ str2 = gcnew String(progBar.c_str());
	String^ str3 = gcnew String(Qbuffer);

	// Percent, Quantity, & Emissions lables Update
	label12->Text = str1;
	label1->Text = str2;
	label4->Text = str3;

}

