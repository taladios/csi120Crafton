// Coal Power Plants (+)
// 4,038,687.23 Metric Tons CO2 per Power Plant
		 const double totalCoal = 1448;
		 double coalsQuantity;
		 int coalPercentBar = 100;
		 double coalsEmish;

	private: System::Void button4_Click(System::Object^  sender, System::EventArgs^  e) {
		double temp;
		if (progressBar2->Value != 100) {
			progressBar2->Value += 10;
			series2->Points->Remove(dataPoint2);
			dataPoint2 = (gcnew System::Windows::Forms::DataVisualization::Charting::DataPoint(0, progressBar2->Value * 2));
			series2->Points->Add(dataPoint2);
			chartArea1->RecalculateAxesScale();
			chart1->Refresh();

			if (TotalPollution->Value != 100) {
				TotalPollution->Value += 2;
			}
			// Percent, Quantity, & Emissions value Update
			coalPercentBar += 20;
			temp = coalPercentBar;
			coalsQuantity = (totalCoal*(temp / 100));
			coalsEmish = (coalsQuantity)*.004039;
		}

		//conversions here
		char Qbuffer[50];
		sprintf(Qbuffer, "%.0f", coalsQuantity);
		char Ebuffer[50];
		sprintf(Ebuffer, "%.3f Billion Metric Tons / year", coalsEmish);
		auto emishBar = std::to_string(coalsEmish);
		String^ str1 = gcnew String(Ebuffer);
		auto progBar = std::to_string(coalPercentBar) + "%";
		String^ str2 = gcnew String(progBar.c_str());
